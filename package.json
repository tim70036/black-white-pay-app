{
  "name": "gs",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "jest": {
    "preset": "jest-expo",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ],
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif|jpg|ttf)$": "RelativeImageStub"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js",
    "web": "PORT=3001 react-scripts start",
    "web-bundle": "react-scripts build",
    "predeploy": "npm run web-bundle",
    "deploy": "gh-pages -d build",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'"
  },
  "dependencies": {
    "@expo/vector-icons": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "expo": "^32.0.5",
    "moment": "^2.24.0",
    "native-base": "^2.11.0",
    "prop-types": "^15.7.2",
    "react": "16.5.0",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-collapsible": "^1.4.0",
    "react-native-easy-toast": "^1.2.0",
    "react-native-elements": "^1.1.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-modal": "^10.0.0",
    "react-native-modal-selector": "^1.0.3",
    "react-native-qrcode": "^0.2.7",
    "react-native-router-flux": "4.0.6",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-tab-view": "^2.4.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.2.2",
    "react-timer-mixin": "^0.13.4",
    "reactstrap": "^7.1.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-expo": "^5.0.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "jest-expo": "^32.0.0",
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.5"
  }
}
